<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="NUnitBenchmarkerConfigSection" type="NUnitBenchmarker.Configuration.NUnitBenchmarkerConfigurationSection, NUnitBenchmarker.Benchmark"/>
  </configSections>

  <NUnitBenchmarkerConfigSection displayUI="true">
    <SearchFolders>
      <add Include ="SFI1" Exclude="SFE1" Folder="SF1" />
      <add Include ="SFI2" Exclude="SFE2" Folder="SF2" />
    </SearchFolders>
    <ImplementationFilters>
      <add Include ="IFI1" Exclude="IFE1" />
      <add Include ="IFI2" Exclude="IFE2" />
    </ImplementationFilters>
    <TestCaseFilters>
      <add Include ="TFI1" Exclude="TFE1" />
      <add Include ="TFI2" Exclude="TFE2" />
    </TestCaseFilters>
  </NUnitBenchmarkerConfigSection>
</configuration>